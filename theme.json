{
  "name": "Deep Water",
  "type": "dark",
  "author": "Christian Naths",
  "colors": {
    "editor.background": "#253349",
    "editor.foreground": "#eceff1",
    "dropdown.background": "#253349",
    "scrollbar.shadow": "#25334900",
    "editorWidget.background": "#253349",
    "editorHoverWidget.background": "#253349",
    "tab.inactiveBackground": "#253349",
    "tab.border": "#253349",
    "editorGroupHeader.tabsBackground": "#253349",
    "panel.border": "#253349",
    "statusBar.background": "#253349",
    "statusBar.noFolderBackground": "#253349",
    "activityBar.background": "#253349",
    "sideBar.background": "#253349",
    "sideBarSectionHeader.background": "#253349",
    "titleBar.activeBackground": "#253349",
    "titleBar.inactiveBackground": "#253349",
    "notification.background": "#253349",
    "editorSuggestWidget.background": "#253349",
    "widget.shadow": "#00000030",
    "list.activeSelectionBackground": "#00000050",
    "list.inactiveSelectionBackground": "#00000050",
    "badge.background": "#00000030",
    "scrollbarSlider.background": "#00000050",
    "scrollbarSlider.hoverBackground": "#00000030",
    "editor.lineHighlightBackground": "#00000050",
    "editorGroup.border": "#00000030",
    "activityBarBadge.foreground": "#000000",
    "editorSuggestWidget.selectedBackground": "#00000050",
    "editorLineNumber.foreground": "#455a64",
    "editor.selectionBackground": "#141B1E",
    "editor.selectionHighlightBackground": "#1d2734",
    "editor.findMatchBackground": "#A9B300",
    "editor.findMatchHighlightBackground": "#1d2734"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#455A64"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#39E67B"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#EA80FC"
      }
    },
    {
      "name": "Library function",
      "scope": [
        "support.function",
        "support.class.component",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#FF4F6C"
      }
    },
    {
      "name": "Meta",
      "scope": ["meta"],
      "settings": {
        "foreground": "#90A4AE"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "meta.import variable.other",
        "meta.definition variable.other",
        "meta.var.expr variable.other.object"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Variable Other",
      "scope": [
        "variable.other",
        "styled js",
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#CFD8DC"
      }
    },
    {
      "name": "Variable Constant",
      "scope": [
        "variable.other.constant.object",
        "variable.other.constant.property"
      ],
      "settings": {
        "foreground": "#FF4F6C"
      }
    },
    {
      "name": "Variable Param",
      "scope": [
        "variable.parameter",
        "support.constant.property-value.css",
        "constant.numeric.css"
      ],
      "settings": {
        "foreground": "#fea349"
      }
    }
  ]
}
